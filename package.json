{
  "name": "ngrx-entity-relationship-react",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.8.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.1.1",
    "@types/jest": "27.5.0",
    "@types/node": "16.11.33",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-redux": "7.1.24",
    "@types/redux": "3.6.0",
    "ngrx-entity-relationship": "1.7.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "7.2.8",
    "react-scripts": "4.0.3",
    "redux": "4.2.0",
    "typescript": "4.6.4",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "prettier": "2.6.2",
    "husky": "7.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "jsx-a11y/aria-role": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%"
    ],
    "development": [
      ">0.2%"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/prettier -u -w ."
    }
  },
  "renovate": {
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)"
    ],
    "prConcurrentLimit": 0,
    "prHourlyLimit": 0,
    "rangeStrategy": "pin",
    "rebaseWhen": "conflicted",
    "separateMajorMinor": true,
    "separateMinorPatch": true,
    "separateMultipleMajor": true,
    "ignoreDeprecated": false,
    "semanticCommitScope": "{{#if baseDir}}{{baseDir}}{{else}}deps{{/if}}",
    "additionalBranchPrefix": "{{#if baseDir}}{{baseDir}}/{{else}}root/{{/if}}",
    "reviewers": [
      "satanTime"
    ],
    "vulnerabilityAlerts": {
      "enabled": false
    }
  }
}

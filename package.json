{
  "name": "ngrx-entity-relationship-react",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.7.2",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.22",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.20",
    "@types/redux": "3.6.0",
    "ngrx-entity-relationship": "1.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-scripts": "4.0.3",
    "redux": "4.1.2",
    "typescript": "4.5.5",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "prettier": "2.5.1",
    "husky": "7.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "jsx-a11y/aria-role": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%"
    ],
    "development": [
      ">0.2%"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/prettier -u -w ."
    }
  },
  "renovate": {
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)"
    ],
    "prConcurrentLimit": 0,
    "prHourlyLimit": 0,
    "rangeStrategy": "pin",
    "rebaseWhen": "conflicted",
    "separateMajorMinor": true,
    "separateMinorPatch": true,
    "separateMultipleMajor": true,
    "ignoreDeprecated": false,
    "semanticCommitScope": "{{#if baseDir}}{{baseDir}}{{else}}deps{{/if}}",
    "additionalBranchPrefix": "{{#if baseDir}}{{baseDir}}/{{else}}root/{{/if}}",
    "reviewers": [
      "satanTime"
    ],
    "vulnerabilityAlerts": {
      "enabled": false
    }
  }
}
